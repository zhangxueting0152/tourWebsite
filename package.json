    "babel-preset-react-app": "^7.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
